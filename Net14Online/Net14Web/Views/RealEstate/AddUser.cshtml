@using Net14Web.Models.RealEstate
@model Net14Web.DbStuff.Models.User

<div>
    <a href="/RealEstate/DataBase">DataBase</a>
</div>
<div class="add-user-block">
    <div class="add-user">
        <form method="post">
            <div>
                Имя
                @Html.TextBoxFor(x => x.Name, new { placeholder =  "Имя" })
            </div>
        
            <div>
                Возраст
                @Html.TextBoxFor(x => x.Age, new { placeholder = "Возраст", @type = "number" })
            </div>
            <div>
                Вид деятельности
                @Html.TextBoxFor(x => x.KindOfActivity, new { placeholder = " Вид деятельности" })
            </div>
            <div>
                <input type="submit" value="Добавить пользователя"/>
            </div>
        </form>
    </div>
</div>