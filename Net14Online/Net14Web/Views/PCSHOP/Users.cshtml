@using Net14Web.Models.PcShop
@model List<UserViewModel>

@{
    Layout = "_LayoutPcShop";
}

<div>
    <table>
        <tr><th>ID</th><th>Имя</th><th>Логин</th><th>Пароль</th></tr>
        @foreach (var User in Model)
        {
            <tr><td>@User.Id</td><td>@User.Name</td><td>@User.Login</td><td>@User.Password</td></tr>
        }
    </table>
</div>