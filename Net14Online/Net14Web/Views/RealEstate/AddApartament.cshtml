@model Net14Web.DbStuff.RealEstate.Models.Apartament

<div>
    <form method="post">
        <div>
            Город
            @Html.TextBoxFor(x => x.City,new {placeholder =  "Город"})
        </div>
        <div>
            Улица
            @Html.TextBoxFor(x => x.Street,new {placeholder =  "Улица"})
        </div>
        <div>
            Номер квартиры 
            @Html.TextBoxFor(x => x.NumberApartament,new {placeholder =  "Номер квартиры "})
        </div>
        <div>
            Количество комнат
            @Html.TextBoxFor(x => x.Size,new {placeholder = "Количество комнат"})
        </div>
        <div>
            <input type="submit" value="Дабавить"/>
        </div>
    </form>
</div>

